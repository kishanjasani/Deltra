{
	"name": "kishan/deltra",
	"description": "A Beautiful WordPress theme packages",
	"type": "wordpress-theme",
	"license": "GPL-3.0+",
	"authors": [
		{
			"name": "Kishan Jasani",
			"email": "kishanjasani007@yahoo.in",
			"role": "Developer"
		}
	],
	"keywords": [
		"WordPress",
		"WordPress Theme"
	],
	"support": {
		"issues": "https://github.com/kishanjasani/Deltra/issues",
		"source": "https://github.com/kishanjasani/Deltra"
	},
	"require": {
		"php": ">=7"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^2.3",
		"phpcompatibility/phpcompatibility-wp": "^2.1"
	},
	"scripts" : {
		"phpcs": "vendor/bin/phpcs",
		"phpcbf": "vendor/bin/phpcbf"
	},
	"autoload": {
		"psr-4": {
			"DELTRA\\Inc\\": "inc/classes/",
			"DELTRA\\Inc\\Traits\\": "inc/traits/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
